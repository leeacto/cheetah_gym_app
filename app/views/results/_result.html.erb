
<tr>
	<td><%= link_to result.user.name, edit_wod_daywod_result_path(result.wod.id, result.daywod_id, result.id)  %></td>
	<% if result.wod.wod_type == "Time" %>
		<td>
			<%= Time.local(1999,1,1,0,
			(result.recd/@wod.baserep).to_i,
			(result.recd - @wod.baserep*(result.recd/@wod.baserep).to_i).to_i).strftime "%M:%S" %>
		</td>
	<% else %>
		<td>
			<%= (result.recd/@wod.baserep).to_i %>
			<% if (result.recd - @wod.baserep*(result.recd/@wod.baserep).to_i).to_i > 0 %>
				+
				<%= (result.recd - @wod.baserep*(result.recd/@wod.baserep).to_i).to_i %>
			<% end %>
			
		</td>
	<% end %>
	<td>
		<% if result.rx  == true %>
		Yes
		<% else %>
		No
		<% end %>

	</td>
	<td><pre><%= result.note %></pre></td>
</tr>
	